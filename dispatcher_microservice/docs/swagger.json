    "host": "192.168.100.16:3000",
    "basePath": "/",
    "paths": {
        "/health": {
            "get": {
                "description": "Return the status of microservice and it's database. Default working state is signified by status \"up\".",
                "tags": [
                    "All microservices"
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/handler.HealthResponse"
                        }
                    },
                    "500": {
                        "description": "Unexpected serverside error",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "handler.HealthResponse": {
            "type": "object",
            "properties": {
                "calculatorMicroservice": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/handler.MicroserviceHealth"
                    }
                },
                "crudMicroservice": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/handler.MicroserviceHealth"
                    }
                },
                "dispatcherStatus": {
                    "type": "string"
                },
                "usersMicroservice": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/handler.MicroserviceHealth"
                    }
                }
            }
        },
        "handler.MicroserviceHealth": {
            "type": "object",
            "properties": {
                "databaseStatus": {
                    "type": "string"
                },
                "microserviceStatus": {
                    "type": "string"
                },
                "microserviceURl": {
                    "type": "string"
                }
            }
        }
    }
